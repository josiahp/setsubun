function particles_init()
	particles_proto={
		update=function(p,dt)
			p.r-=4*dt
			if p.r<=0 then
				p.is_done=true
			end
		end,
		draw=function(p)
			circfill(p.x,p.y,p.r,7)
			circ(p.x,p.y,p.r,6)
		end,
	}
	particles_meta={__index=particles_proto}
end

function particle_new(x,y)
	local particle={
		x=x,
		y=y,
		r=4,
		is_done=false,
	}
	setmetatable(particle,particles_meta)
	return particle
end

